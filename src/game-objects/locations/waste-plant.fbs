@LOCATION WASTE PLANT

:.backgroundColor (( #FFED90 ))
:.textColor ((#341139))
:.backgroundTextColor ((#359668))

START >>
- $THIS.name <- "Waste Plant"
- $THIS.idx <- 12
- $THIS add CLUB
- $THIS add SETTLEMENT
- $THIS add ARENA
- $THIS add <POWER STATION>
- $THIS add EMPTY

LOOK >>
- ((This here is waste plant. {? $THIS.running is true}
  It's a churnin'.{x}
  {? $THIS.running not true}It's stopped.{x}))
